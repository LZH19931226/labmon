<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hc.clickhouse.mapper.MonitorequipmentlastdataMapper">

    <select id="getMonitorEquipmentLastDataInfo" resultType="com.hc.clickhouse.po.Monitorequipmentlastdata">
        select
            *
        from
            monitorequipmentlastdata
        where
            formatDateTime(inputdatetime,'%Y-%m-%d') = #{date}
         and equipmentno = #{equipmentNo}
          ORDER BY inputdatetime ASC
    </select>


</mapper>
